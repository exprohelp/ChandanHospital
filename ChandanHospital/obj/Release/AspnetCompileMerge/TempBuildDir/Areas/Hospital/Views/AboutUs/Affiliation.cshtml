<script>
	$(function () {
		BindCorporate();
	});
	function BindCorporate() {
		$.ajax({
			type: "POST",
			url: "affiliation.aspx/BindCorporate",
			data: '{}',
			contentType: "application/json;charset=utf-8",
			datatype: "jsondata",
			success: function (data) {
				$('#tbodyTable').empty();
				if (data.d.length > 0) {
					var temp = "";
					for (var i = 0; i < data.d.length; i++) {

						if (temp != data.d[i].Corporate_Type) {

							if (data.d[i].Corporate_Type == "COR_COM") {

								$("#tbodyTable").append('<tr style="background: #0ab1b9; color: white;text-transform:uppercase;"><th colspan="2" style="text-align: left;">List of empanelled corporate companies</th></tr>');

							} else {
								$("#tbodyTable").append('<tr style="background: #0ab1b9; color: white;text-transform:uppercase;"><th colspan="2" style="text-align: left;">List of empanelled TPA/Insurance companies</th></tr>');
							}

							temp = data.d[i].Corporate_Type;
						}

						$("#tbodyTable").append(
							'<tr>' +
							'<td>' + (i + 1) + '</td>' +
							'<td style="text-align: left;">' + data.d[i].Corporate_Name + '</td>' +
							'</tr>');
					}
				}
				else {
					$("#tbodyTable").append('<tr><td colspan="2">No Records Found</td></tr>');
				}
			}
		});
	}
</script>

