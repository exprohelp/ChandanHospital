
@{
    Layout = null;
}
<meta name="viewport" content="width=device-width,initial-scale=1.0" />
<meta http-equiv="content-type" content="text/html; charset=UTF-8" />
<link href="/Content/bootstrap.min.css" rel="stylesheet" />
<script src="~/Content/js/jquery-2.2.4.min.js"></script>
<script src="~/JsModule/global.js"></script>
<script src="~/Areas/Hospital/JsModule/WhatWeTreat.js"></script>
<style type="text/css">
    .panel {
        margin-bottom: 5px;
    }

    .panel-heading {
        padding: 8px 5px;
    }

    hr {
        margin-top: 2px !important;
        margin-bottom: 2px !important;
        border: 1px solid #0ab1b9 !important;
    }

    p {
        margin: 0 0 0 0 !important;
    }

    h3, h4, h5, h6, .h3, .h4, .h5, .h6 {
        margin-top: 0 !important;
        margin-bottom: 0 !important;
        font-size: 16px;
        color: #000;
    }

    a {
        color: #1588dc;
        padding: 10px;
    }

    p {
        color: #3c3b3b;
        font-size: 12px
    }

    img {
        margin: 8px 0 0 0;
    }

    .btn-group-xs > .btn, .btn-xs {
        margin: -18px 0 0 0;
    }
</style>

<div class="container">
    <div class="row">
        <div class="col-lg-12">        
            <div class="panel panel-primary">              
                <div class="panel-heading">
                    <h3 class="text-center" id="lbldeptname"></h3>
                    <input type="button" value="Back" id="btngotoweb" class="btn btn-danger btn-flat btn-xs pull-right" />
                </div>
                <div class="panel-body" id="divWhatWeTreat">                
                </div>              
            </div>           
        </div>
    </div>    
</div>

<script type="text/javascript">
    $("#btngotoweb").click(function () {
        window.history.back();
    });
</script>





